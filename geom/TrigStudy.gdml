    <volume name="log_outRock">
      <materialref ref="Rock"/>
      <solidref ref="out_Rock2"/>
    </volume>
    <volume name="log_innRock">
      <materialref ref="Rock"/>
      <solidref ref="inn_CutOut"/>
    </volume>
    <volume name="log_World">
      <materialref ref="G4_AIR"/>
      <solidref ref="sol_World"/>
