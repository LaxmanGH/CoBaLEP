      <first ref="sol_Rock"/>
      <second ref="sol_Lab"/>
      <position name="out_Rock_pos" unit="mm" x="-2500" y="0" z="4000"/>
    </subtraction>
    <tube aunit="deg" deltaphi="360" lunit="mm" name="inn_CutOut" rmax="11500.01" rmin="0" startphi="0" z="23000.02"/>
